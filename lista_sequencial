#include<stdio.h>

int A=10, B=18, X; 

int main(){
	
	printf("O valor de A: \n");
	scanf("%d", &A);
	printf("O valor de B: \n");
	scanf("%d", &B);
	
	printf("O valor de A %d + o valor de B %d e igual a %d", A, B, X);
	
}

