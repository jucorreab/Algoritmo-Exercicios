#include<stdio.h>
int N1, N2;

int main(){
	printf("Digite o numero 1: \n");
	scanf("%d", &N1);
	printf("Digite o numero 2: \n");
	scanf("%d", &N2);
	
	if (N1 > N2){
		printf("O numero %d e o maior. \n", N1);
	}
	else if (N2 > N1){
		printf("O numero %d e o maior. \n", N2);
	}
}

